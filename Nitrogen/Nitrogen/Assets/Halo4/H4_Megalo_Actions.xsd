                            <xs:sequence>
                              <xs:element name="Constraints">
                                <xs:complexType>
                                  <xs:sequence>
                                    <xs:element name="MinValueConstraint" type="xs:decimal" />
                                    <xs:element name="MaxValueConstraint" type="xs:decimal" />
                                  </xs:sequence>
                                </xs:complexType>
                              </xs:element>
                            </xs:sequence>
                            <xs:attribute name="Size" type="xs:unsignedByte" use="required" />
                            <xs:attribute name="Unsigned" type="xs:string" use="required" />
                            <xs:attribute name="FloatFlag" type="xs:string" use="required" />
                            <xs:attribute name="Name" type="xs:string" use="optional" />
                          </xs:complexType>
                        </xs:element>
                        <xs:element name="StringReference">
                          <xs:complexType>
                            <xs:attribute name="Name" type="xs:string" use="required" />
                          </xs:complexType>
                        </xs:element>
                      </xs:choice>
                    </xs:sequence>
                  </xs:complexType>
                </xs:element>
                <xs:element name="Description" type="xs:string" />
                <xs:element name="Remarks" type="xs:string" />
                <xs:element name="Deprecated" />
                <xs:element name="PlayerReference" />
                <xs:element name="IntegerReference">
                  <xs:complexType>
                    <xs:sequence minOccurs="0">
                      <xs:element name="Constraints">
                        <xs:complexType>
                          <xs:sequence>
                            <xs:element name="MinValueConstraint" type="xs:unsignedByte" />
                            <xs:element name="MaxValueConstraint" type="xs:unsignedByte" />
                          </xs:sequence>
                        </xs:complexType>
                      </xs:element>
                    </xs:sequence>
                  </xs:complexType>
                </xs:element>
                <xs:element name="Integer">
                  <xs:complexType>
                    <xs:sequence minOccurs="0">
                      <xs:element name="Constraints">
                        <xs:complexType>
                          <xs:sequence>
                            <xs:element minOccurs="0" name="EnumConstraint" type="xs:string" />
                            <xs:element minOccurs="0" name="IndexOfConstraint" type="xs:string" />
                          </xs:sequence>
                        </xs:complexType>
                      </xs:element>
                    </xs:sequence>
                    <xs:attribute name="Size" type="xs:unsignedByte" use="required" />
                    <xs:attribute name="Unsigned" type="xs:string" use="required" />
                    <xs:attribute name="Name" type="xs:string" use="required" />
                    <xs:attribute name="Nullable" type="xs:string" use="optional" />
                  </xs:complexType>
                </xs:element>
                <xs:element name="StringReferenceMultiToken" />
                <xs:element name="TeamReference" />
                <xs:element name="PossiblyDeprecated" />
              </xs:choice>
            </xs:sequence>
            <xs:attribute name="Opcode" type="xs:unsignedByte" use="required" />
            <xs:attribute name="Name" type="xs:string" use="optional" />
          </xs:complexType>
        </xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
</xs:schema>